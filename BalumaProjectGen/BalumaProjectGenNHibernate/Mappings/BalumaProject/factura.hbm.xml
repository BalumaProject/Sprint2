<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="BalumaProjectGenNHibernate"
		                   namespace="BalumaProjectGenNHibernate.EN.BalumaProject">
			<class name="FacturaEN" table="[Factura]">
						<id name="Id" column="[id]"/>
					<property name="HTML" type="string"> 
						<column name="[HTML]"  not-null="true"/>
					</property>
			
			
			
		  			
						<bag name="Pedido" table="[pedido_factura]" inverse="true" >
							<key>
									<column name="[FK_id_idFactura]"/>
							</key>
      						<many-to-many class="BalumaProjectGenNHibernate.EN.BalumaProject.PedidoEN">
									<column name="[FK_idPedido_idPedido]"/>
      						</many-to-many>
    					</bag>
					
					
					
		
		  </class>
		  
		  <query name="FacturaENReadAllHQL">from FacturaEN</query>
		</hibernate-mapping>
	